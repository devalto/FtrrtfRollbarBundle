{
    "name": "ftrrtf/rollbar-bundle",
    "description": "Symfony2 Bundle for rollbar.com",
    "keywords": ["rollbar", "errors", "symfony2"],
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Valeriy Rabievskiy",
            "email": "dev@ftrrtf.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/framework-bundle": "^2.7.3|^3.0|^4.0",
        "symfony/security-bundle": "^2.7.3|^3.0|^4.0",
        "symfony/expression-language": "^2.7.3|^3.0|^4.0",
        "ftrrtf/rollbar-notifier": "dev-symfony4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "phpspec/phpspec": "^2.4",
        "symfony/twig-bundle": "^2.7.3|^3.0|^4.0",
        "mikey179/vfsstream": "^1.6"
    },
    "suggest": {
        "symfony/twig-bundle": "Enable rollbarjs twig helpers"
    },
    "autoload": {
        "psr-4": {
            "Ftrrtf\\RollbarBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/devalto/rollbar-notifier",
            "no-api": true
        }
    ]
}
